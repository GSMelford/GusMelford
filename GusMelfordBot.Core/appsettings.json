 {
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
   "Serilog": {
     "Using": [ "Serilog.Sinks.Graylog" ],
     "MinimumLevel": {
       "Default": "Information",
       "Override": {
         "Microsoft": "Error",
         "System": "Information"
       }
     },
     "WriteTo": [
       {
         "Name": "Graylog",
         "Args": {
           "hostnameOrAddress": "192.168.31.110",
           "port": "12201",
           "transportType": "Udp"
         }
       }
     ],
     "Properties": {
       "Application": "Sakura"
     }
   },
   "AllowedHosts": "*"
}

