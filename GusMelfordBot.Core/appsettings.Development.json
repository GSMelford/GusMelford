{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CommonSettings": {
    "Name": "GusMelfordBot",
    "Version": "",
    "PlayerVersion": "",
    "TelegramBotSettings": {
      "Token": ""
